{
	"expo": {
		"name": "Stephagramme",
		"slug": "stephagramme",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "stephagramme",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": false,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#222a22"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.marine.stephagramme",
			"versionCode": 1
		},
		"web": {
			"output": "static"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#222a22",
					"dark": {
						"backgroundColor": "#0b0b0d"
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {}
		}
	}
}
